<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="view" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="desdeColumn" prefWidth="75.0" text="Desde" />
          <TableColumn fx:id="hastaColumn" prefWidth="75.0" text="Hasta" />
            <TableColumn fx:id="denominacionColumn" prefWidth="100.0" text="Denominación" />
            <TableColumn fx:id="organizadorColumn" prefWidth="100.0" text="Organizador" />
        </columns>
      </TableView>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButton" prefWidth="75.0" text="Añadir" />
            <Button fx:id="eliminarButton" mnemonicParsing="false" onAction="#onEliminarAction" prefWidth="75.0" text="Eliminar" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
